{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext", "DOM"],
		"types": ["vite/client"],
		"paths": {
      "@/*": ["./app/*"]
    },
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["**/*.ts", "**/*.tsx"]
}
